# Add deb-src source for developers
echo "deb-src ${EOB_OS_REPO} ${EOB_BRANCH} ${EOB_OS_COMPONENTS//,/ }" \
  >> "${EOB_ROOTDIR}"/etc/apt/sources.list

echo "deb http://deb.debian.org/debian buster-backports main contrib non-tree" \
       >> "${EOB_ROOTDIR}"/etc/apt/sources.list
       
echo "deb-src http://deb.debian.org/debian buster-backports main contrib non-tree" \
       >> "${EOB_ROOTDIR}"/etc/apt/sources.list
